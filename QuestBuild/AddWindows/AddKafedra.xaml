<Window x:Class="QuestBuild.AddWindows.AddKafedra"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="AddKafedra" Height="110" Width="413">
    <Grid>
        <Label Content="Кафедра:" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top" Width="70"/>
        <TextBox x:Name="TextBox_Kafedra" HorizontalAlignment="Left" Height="26" Margin="85,41,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="191"/>
        <Button x:Name="Button_SaveKafedra" Content="Добавить" HorizontalAlignment="Left" Margin="281,41,0,0" VerticalAlignment="Top" Width="114" Height="26" Click="Button_SaveKafedra_Click"/>
        <Label Content="Факультет:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="70"/>
        <ComboBox x:Name="Cb_Fakultet" HorizontalAlignment="Left" Margin="85,10,0,0" VerticalAlignment="Top" Width="191" Height="26" DropDownClosed="Cb_Fakultet_DropDownClosed"/>
        <Button x:Name="Button_AddFakultet" Content="Добавить" HorizontalAlignment="Left" Margin="281,10,0,0" VerticalAlignment="Top" Width="114" Height="26" Click="Button_AddFakultet_Click"/>

    </Grid>
</Window>
